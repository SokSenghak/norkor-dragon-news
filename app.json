{
  "expo": {
    "name": "Norkor Dragon News",
    "slug": "norrkor-dragon-news",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/newspaper2.png",
    "scheme": "rork-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/newspaper2.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B4A7C"
    },
    "ios": {
      "sound": true,
      "assets": [
        "./assets/audio/notification.mp3"
      ],
      "supportsTablet": true,
      "bundleIdentifier": "com.inoover.NokorDragonNews",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "sound": true,
      "assets": [
        "./assets/audio/sound.mp3"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/newspaper2.png",
        "backgroundColor": "#2B4A7C"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.inoover.nokorwatnews",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router", { "origin": "https://rork.com/" }],
      "expo-font",
      "expo-web-browser",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "android": {
          "compileSdkVersion": 36,
          "targetSdkVersion": 34,
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}